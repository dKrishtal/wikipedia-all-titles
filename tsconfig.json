  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": false,
    "target": "es2017",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es7"
    ],
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": "."
  },
  "exclude": [
    "node_modules"
  ]
}